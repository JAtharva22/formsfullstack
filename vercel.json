{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/admin_view/(.*)",
      "dest": "admin_view.html"
    },
    {
      "src": "/customer_form/(.*)",
      "dest": "customer_form.html"
    }
  ]
}
